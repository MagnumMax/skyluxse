{
  "name": "skyluxse",
  "version": "1.0.0",
  "description": "Современное веб-приложение для управления автопарком с интерактивным интерфейсом.",
  "main": "next.config.mjs",
  "scripts": {
    "dev": "PORT=${PORT:-6767}; lsof -ti:${PORT} | xargs kill -9 || true; next dev -p ${PORT} --webpack",
    "dev:turbopack": "PORT=${PORT:-6767}; lsof -ti:${PORT} | xargs kill -9 || true; next dev -p ${PORT}",
    "build": "next build --webpack",
    "build:turbopack": "next build",
    "start": "PORT=${PORT:-6767} next start -p ${PORT}",
    "lint": "eslint . --report-unused-disable-directives --max-warnings=0",
    "typecheck": "tsc --noEmit",
    "test:unit": "tsx --test --tsconfig tsconfig.json --import ./tests/setup.ts tests/unit/calendar-grid.test.ts tests/unit/fleet-booking-utils.test.ts tests/unit/sanity.test.mjs tests/unit/zoho-sales-order-payload.test.ts",
    "test:watch": "tsx --test --watch --tsconfig tsconfig.json --import ./tests/setup.ts \"tests/unit/**/*.test.ts\" \"tests/unit/**/*.test.tsx\" \"tests/unit/**/*.test.mjs\"",
    "test:component": "vitest run",
    "test:cleanup": "tsx scripts/cleanup-test-data.ts",
    "test:e2e": "npx playwright test",
    "test": "npm run test:unit && npm run test:component && npm run test:e2e",
    "check": "npm run lint && npm run typecheck && npm run test",
    "predeploy": "npm run check",
    "shadcn": "shadcn",
    "vercel": "vercel build --local-config vercel.json --yes",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MagnumMax/skyluxse.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "bugs": {
    "url": "https://github.com/MagnumMax/skyluxse/issues"
  },
  "homepage": "https://github.com/MagnumMax/skyluxse#readme",
  "devDependencies": {
