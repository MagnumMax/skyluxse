{
  "name": "skyluxse",
  "version": "1.0.0",
  "description": "Современное веб-приложение для управления автопарком с интерактивным интерфейсом.",
  "main": "next.config.mjs",
  "scripts": {
    "dev": "lsof -ti:3000 | xargs kill -9 || true && next dev",
    "dev:webpack": "NEXT_DISABLE_TURBOPACK=1 next dev",
    "build": "next build",
    "build:webpack": "next build --webpack",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx --report-unused-disable-directives --max-warnings=0",
    "typecheck": "tsc --noEmit",
    "test:unit": "tsx --test --tsconfig tsconfig.json --import ./tests/setup.ts tests/unit/fleet-booking-utils.test.ts tests/unit/sanity.test.mjs",
    "test:watch": "tsx --test --watch --tsconfig tsconfig.json --import ./tests/setup.ts \"tests/unit/**/*.test.ts\" \"tests/unit/**/*.test.tsx\" \"tests/unit/**/*.test.mjs\"",
    "test:component": "vitest run",
    "test:e2e": "playwright test",
    "test": "npm run test:unit && npm run test:component",
    "check": "npm run lint && npm run typecheck && npm run test",
    "predeploy": "npm run check && npm run test:e2e",
    "shadcn": "shadcn-ui",
    "vercel": "vercel build --local-config vercel.json --yes"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MagnumMax/skyluxse.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "bugs": {
    "url": "https://github.com/MagnumMax/skyluxse/issues"
  },
  "homepage": "https://github.com/MagnumMax/skyluxse#readme",
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@tailwindcss/postcss": "^4.1.17",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
    "@vitest/coverage-v8": "^4.0.9",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.38.0",
    "eslint-config-next": "^16.0.1",
    "happy-dom": "^20.0.10",
    "postcss": "^8.4.49",
    "prettier": "^3.6.2",
    "puppeteer": "^24.30.0",
    "shadcn-ui": "^0.9.5",
    "tailwindcss": "^4.1.17",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vercel": "^48.10.0",
    "vitest": "^4.0.9"
  },
  "dependencies": {
    "@hello-pangea/dnd": "^18.0.1",
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@supabase/supabase-js": "^2.80.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "critters": "^0.0.22",
