  "main": "sw.js",
  "scripts": {
    "build-css": "npx tailwindcss -i ./src/input.css -o ./dist/output.css",
    "build": "npm run build-css && cp index.html dist/",
    "dev": "npx tailwindcss -i ./src/input.css -o ./dist/output.css --watch",
