            "enabled_tools": ["open", "screenshot"],
            "disabled_tools": ["screenshot"],
            "startup_timeout_sec": 20,
            "tool_timeout_sec": 45,
            "enabled": true
        },
        "shadcn": {
            "command": "npx",
            "args": ["shadcn@latest", "mcp"]
        }
    }
